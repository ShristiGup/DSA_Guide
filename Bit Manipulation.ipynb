    "def min_xor(arr):\n",
    "    n = len(arr)\n",
    "    arr.sort()\n",
    "    m = (2**31)+1\n",
    "    for i in range(0,n-1):\n",
    "        xor = arr[i]^arr[i+1]\n",
    "        if xor<m:\n",
    "            m = xor\n",
    "            l = []\n",
    "            l.append([arr[i],arr[i+1]])\n",
    "        elif xor==m:\n",
    "            l.append([arr[i],arr[i+1]])\n",
    "    print(m)\n",
    "    return l\n",
