    "# All repeating except two\n",
    "def two_no(arr):\n",
    "    xor = 0\n",
    "    for i in range(len(arr)):\n",
    "        xor = xor^arr[i]\n",
    "    num = xor & -xor\n",
    "    x = 0\n",
    "    y = 0\n",
    "    for i in range(len(arr)):\n",
    "        if((num & arr[i])==0):\n",
    "            x = x^arr[i]\n",
    "        else:\n",
    "            y = y^arr[i]\n",
    "    n1 = min(x,y)\n",
    "    n2 = max(x,y)\n",
    "    return [n1,n2]\n",
    "    \n",
    "arr = list(map(int,input().split()))\n",
    "ans = two_no(arr)\n",
    "print(*ans,sep=\" \")"
   ]
