    "# Right-most set bit position\n",
    "import math\n",
    "def rsb(n):\n",
    "    ans = n & -n\n",
    "    if ans:\n",
    "        p = int(math.log2(ans))\n",
    "        return p+1\n",
    "    return 0\n",
    "    \n",
    "n = int(input())\n",
    "print(rsb(n))"
   ]
