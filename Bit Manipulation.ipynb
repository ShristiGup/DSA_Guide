    "# Count set bits in an integer(Kernighan's Algorithm)\n",
    "def count_set_bits(n):           # T.C. - O(logn)\n",
    "    c = 0\n",
    "    while(n!=0):\n",
    "        rsb_mask = n & -n\n",
    "        n-=rsb_mask\n",
    "        c+=1\n",
    "    return c\n",
    "\n",
    "n = int(input())\n",
    "print(count_set_bits(n))"
   ]
