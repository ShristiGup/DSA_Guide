    "# Josephus problem\n",
    "import math\n",
    "def jos(n):\n",
    "    p = int(math.log(n,2))\n",
    "    num = 2**p\n",
    "    l = n-num\n",
    "    ans = 2*l+1\n",
    "    return ans\n",
    "    \n",
    "n = int(input())\n",
    "print(jos(n))"
   ]
